<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  
  <servlet>
    <servlet-name>PineEditorServlet</servlet-name>
    <servlet-class>org.latexlab.pine.server.PineEditorServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>PineExplorerServlet</servlet-name>
    <servlet-class>org.latexlab.pine.server.PineExplorerServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>GDataServlet</servlet-name>
    <servlet-class>org.latexlab.pine.server.GDataServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>PineEditorServlet</servlet-name>
    <url-pattern>/docs/*</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>PineExplorerServlet</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>GDataServlet</servlet-name>
    <url-pattern>/org.latexlab.pine.editor.PineEditor/data</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>GDataServlet</servlet-name>
    <url-pattern>/org.latexlab.pine.explorer.PineExplorer/data</url-pattern>
  </servlet-mapping>
  
</web-app>
